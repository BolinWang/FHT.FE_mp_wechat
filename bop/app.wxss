/**app.wxss**/
@import 'vendors/zanui-weapp/dist/index.wxss';
/* pop */
.pop--top {
  width: 100vw;
  padding: 15rpx;
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}

.pop__container {
  overflow: hidden;
  background: #fff;
}

.pop__container--bottom {
  width: 100vw;
}

.pop__container--left, .pop__container--right {
  height: 100vh;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  color: #666;
} 

.flex {
  display: flex;
}

.relative {
  position: relative;
}

.absolute {
  position: absolute;
}

.align-center {
  text-align: center;
}

.align-left {
  text-align: left;
}

.align-right {
  text-align: right;
}

.left {
  float: left;
}

.right {
  float: right;
}

.mainColor {
  color: #FF9800;
}

.mainBgcolor {
  background-color: #FF9800;
}

.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:48rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: #999;
}

.icon-roundcheck:before { content: "\e657"; }

.icon-roundclose:before { content: "\e659"; }

.icon-back:before { content: "\e679"; }

.icon-right:before { content: "\e6a3"; }

.icon-homefill:before { content: "\e6bb"; }

.icon-friendaddfill:before { content: "\e6c9"; }

.icon-mima:before { content: "\e694"; }

.icon-biaoji:before { content: "\e708"; }

.icon-yinxingqia:before { content: "\e683"; }

.icon-fapiaoshenhe:before { content: "\e63c"; }

.icon-shenfenzheng:before { content: "\e625"; }

.icon-wode:before { content: "\e62f"; }

.icon-shouye:before { content: "\e62e"; }

.icon-sousuo:before { content: "\e632"; }

.icon-guanyanren:before { content: "\e63f"; }

.icon-iphone:before { content: "\e640"; }

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}


